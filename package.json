{
  "name": "webpack-library",
  "version": "1.0.0",
  "description": "Produce universal library with webpack and es6",
  "main": "lib/webpack-library.js",
  "scripts": {
    "dev": "webpack --progress --watch",
    "build": "yarn test && webpack --progress && cross-env NODE_ENV=production webpack --progress",
    "test": "./node_modules/.bin/jest",
    "test-watch": "./node_modules/.bin/jest --watch",
    "repl": "node -i -e \"$(< ./lib/webpack-library.js)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeselvmo/webpack-library.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs",
    "jest"
  ],
  "author": "kecoyo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeselvmo/webpack-library/issues"
  },
  "homepage": "https://github.com/jeselvmo/webpack-library",
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-typescript": "^7.16.0",
    "babel-jest": "26.6.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-add-module-exports": "1.0.4",
    "cross-env": "^7.0.3",
    "jest": "26.6.3",
    "mocha": "^4.0.1",
    "webpack": "5.11.0",
    "webpack-cli": "4.2.0",
    "yargs": "^10.0.3"
  }
}

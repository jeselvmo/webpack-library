{
  "name": "webpack-library",
  "library": "WebpackLibrary",
  "version": "1.0.0",
  "description": "Produce universal library with webpack and es6",
  "main": "dist/webpack-library.js",
  "scripts": {
    "start": "webpack --progress --watch",
    "build": "webpack --progress && cross-env NODE_ENV=production webpack --progress",
    "server": "browser-sync start --server --files 'dist/*.js'",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeselvmo/webpack-library.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "library",
    "umd",
    "commonjs",
    "jest"
  ],
  "author": "kecoyo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeselvmo/webpack-library/issues"
  },
  "homepage": "https://github.com/jeselvmo/webpack-library",
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "babel-jest": "27.3.1",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "jest": "27.3.1",
    "webpack": "5.62.1",
    "webpack-cli": "4.9.1"
  }
}
